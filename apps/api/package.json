{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate --schema=src/prisma/schema.prisma",
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "db:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "db:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "db:push": "prisma db push --schema=src/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "db:seed": "tsx src/prisma/seed.ts"
  },
  "dependencies": {
    "@image-compressor/shared": "workspace:*",
    "@prisma/client": "^6.3.1",
    "@supabase/supabase-js": "^2.98.0",
    "archiver": "^7.0.1",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.34.8",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sharp": "^0.33.5",
    "stripe": "^17.6.0",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.9",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "prisma": "^6.3.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
